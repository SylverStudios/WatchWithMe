{
  "manifest_version": 2,

  "name": "Watch With Me",
  "description": "This displays additional buttons in the UI to control videos",
  "version": "1.0",

  "content_scripts": [
    {
      "matches" : ["https://*/*","file:///*/*"],
      "js": ["library/jquery1.9.min.js", "contentScript.js"]
    }
  ],

  "background": {
    "scripts": ["library/jquery1.9.min.js", "background.js"]
  },

  "browser_action": {
    "default_icon": "image/tempImage.png",
    "default_popup": "watchWithMebaseTest.html",
    "default_title": "That syncing feeling"
  },

  "permissions": [
    "tabs",
    "activeTab",
    "https://*/",
    "file:///*/*",
    "https://ajax.googleapis.com/"
  ]
}